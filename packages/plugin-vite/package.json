{
	"name": "@giga/plugin-vite",
	"version": "0.1.0",
	"description": "Vite plugin for Giga",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/WLowe10/giga",
		"type": "git",
		"directory": "packages/plugin-vite"
	},
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "tsup ./src/index.ts --dts --format=esm,cjs"
	},
	"dependencies": {
		"@babel/core": "^7.24.6",
		"giga": "workspace:*"
	},
	"peerDependencies": {
		"vite": "^4.2.0 || ^5.0.0"
	},
	"devDependencies": {
		"@types/babel__core": "^7.20.5",
		"tsup": "^8.0.2"
	}
}
